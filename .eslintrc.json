{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2021
    },
    "rules": {
      "react/react-in-jsx-scope": 0
    }
}